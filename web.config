<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpProtocol>
            <customHeaders>
                <remove name="X-Powered-By" />
                <add name="Cache-Control" value="must-revalidate" />
            </customHeaders>
        </httpProtocol>
		<staticContent>
			<clientCache cacheControlMode="DisableCache" cacheControlMaxAge="01:00:00" />
		</staticContent>
    </system.webServer>
</configuration>
